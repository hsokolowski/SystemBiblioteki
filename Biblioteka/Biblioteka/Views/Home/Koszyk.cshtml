@model IEnumerable<Biblioteka.Models.Book>
@{
    ViewBag.Title = "Koszyk";
    var lista = Session["Zamowienie"] as IEnumerable<Biblioteka.Models.Book>;
}

<h2>Koszyk</h2>

<div>
    <h1>Twój koszyk</h1>
    @if (lista != null)
    {
        foreach (var u in lista)
        {
            if (Session["login"] != null)
            {
                @u.id_book @u.title
            }

        }
        @Html.ActionLink("Wypożycz", "Borrow", new { }) // liste przekazac / odebrac w kontrolerze / dodac do borrowingow i wypisac
    }


</div>